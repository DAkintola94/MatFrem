@model MatFrem.Models.ViewModel.EditViewTable

<link href="~/css/homeStyle.css" rel="stylesheet" />

<h1> Kart rapport oversikt </h1>
<div class="container py-5">
	<form method="post" asp-controller="Location" asp-action="ViewIndexTable">
		<div class="mb-3">
			<label class="form-label"> Rapport ID </label>
			<input type="text" class="form-control" asp-for="LocationReportID" readonly /> <!-- the value in name must match the model's property -->
		</div>

		<div class="mb-3">
			<label class="form-label"> Geo json </label>
			<input type="text" class="form-control" asp-for="GeoJson" readonly /> <!-- the value in name must match the model's property -->
		</div>

		<div class="mb-3">
			<label class="form-label"> Dato </label>
			<input type="text" class="form-control" asp-for="Date" /> <!-- the value in name must match the model's property -->
		</div>

		<div class="mb-3">
			<label class="form-label"> Melding </label>
			<input type="text" class="form-control" asp-for="LocationMessage" /> <!-- the value in name must match the model's property -->
		</div>

		<div class="mb-3">
			<div class="d-flex">
				<button type="submit" class="btn btn-dark">
					Update
				</button>
			</div>
		</div>

	</form>
</div>